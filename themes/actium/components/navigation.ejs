        <script>
// Select the element that you want to track clicks on
const myElement = document.getElementById('coins');

// Set a click counter to 0
let clickCounter = 0;

// Add a click event listener to the element
myElement.addEventListener('click', function() {
  // Increase the click counter by 1
  clickCounter++;

  // Check if the click counter is equal to 5
  if (clickCounter === 5) {
    // Redirect the user to the desired URL
    window.location.href = '../coins';
  }
});
